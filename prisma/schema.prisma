model customer_user {
  id                Int      @id @default(autoincrement())
  login             String
  email             String
  customer_id       String
  pw                String
  title             String
  first_name        String
  last_name         String
  phone             String
  fax               String
  mobile            String
  street            String
  zip               String
  city              String
  country           String
  comments          String
  telegram_username String
  valid_id          String
  create_time       DateTime
  create_by         Int
  change_time       String
  change_by         Int
}

model service {
  id                    Int                     @id @default(autoincrement())
  name                  String
  valid_id              String
  comments              String
  create_time           String
  create_by             Int
  change_time           String
  change_by             DateTime
  type_id               String
  criticality           String
  service_preferences   service_preferences[]
  personal_services     personal_services[]
  service_customer_user service_customer_user[]
  service_sla           service_sla[]
  ticket                ticket[]
}

model service_preferences {
  id Int @id @default(autoincrement())

  service_id        Int
  preferences_key   String
  preferences_value String

  service service @relation(fields: [service_id], references: [id])
}

model service_sla {
  id Int @id @default(autoincrement())

  service_id Int
  sla_id     Int

  service service @relation(fields: [service_id], references: [id])
  sla     sla     @relation(fields: [sla_id], references: [id])
}

model sla {
  id                     Int           @id @default(autoincrement())
  name                   String
  calendar_name          String
  first_response_time    Int
  first_response_notify  Int
  update_time            Int
  update_notify          Int
  solution_time          Int
  solution_notify        Int
  valid_id               Int
  comments               String
  create_time            DateTime
  create_by              Int
  change_time            DateTime
  change_by              Int
  type_id                Int
  min_time_bet_incidents Int
  service_sla            service_sla[]
  ticket                 ticket[]
}

model personal_services {
  id Int @id @default(autoincrement())

  service_id Int
  user_id    Int

  service service @relation(fields: [service_id], references: [id])
  users   users   @relation(fields: [user_id], references: [id])
}

model service_customer_user {
  id Int @id @default(autoincrement())

  service_id          Int
  customer_user_login String
  create_time         DateTime
  create_by           Int

  service service @relation(fields: [service_id], references: [id])
  users   users   @relation(fields: [customer_user_login], references: [login])
}

model users {
  id          Int      @id @default(autoincrement())
  login       String   @unique
  pw          String
  title       String
  first_name  String
  last_name   String
  valid_id    String
  create_time DateTime
  create_by   Int
  change_time String
  change_by   Int

  personal_services     personal_services[]
  service_customer_user service_customer_user[]
  ticket                ticket[]                @relation("userTickets")
  ticketsResponsable    ticket[]                @relation("responsibleTickets")
  queue                 queue[]
  ticket_lock_type      ticket_lock_type[]
  ticket_type           ticket_type[]
  ticket_priority       ticket_priority[]
  ticket_state          ticket_state[]
  follow_up_possible    follow_up_possible[]
  permission_groups     permission_groups[]
  valid                 valid[]
  salutation            salutation[]
  signature             signature[]
  system_address        system_address[]
}

model ticket {
  id                       Int      @id @default(autoincrement())
  tn                       String
  title                    String
  queue_id                 Int
  ticket_lock_id           Int
  type_id                  Int
  service_id               Int
  sla_id                   Int
  user_id                  Int
  responsible_user_id      Int
  ticket_priority_id       Int
  ticket_state_id          Int
  customer_id              String
  customer_user_id         String
  timeout                  Int
  until_time               Int
  escalation_time          Int
  escalation_update_time   Int
  escalation_response_time Int
  escalation_solution_time Int
  archive_flag             Int
  create_time              DateTime
  create_by                Int
  change_time              DateTime
  change_by                Int

  service          service          @relation(fields: [service_id], references: [id])
  ticket_state     ticket_state     @relation(fields: [ticket_state_id], references: [id])
  queue            queue            @relation(fields: [queue_id], references: [id])
  ticket_lock_type ticket_lock_type @relation(fields: [ticket_lock_id], references: [id])
  ticket_type      ticket_type      @relation(fields: [type_id], references: [id])
  sla              sla              @relation(fields: [sla_id], references: [id])
  users            users            @relation("userTickets", fields: [user_id], references: [id])
  responsible      users            @relation("responsibleTickets", fields: [responsible_user_id], references: [id])
  ticket_priority  ticket_priority  @relation(fields: [ticket_priority_id], references: [id])
}

model queue {
  id                    Int      @id @default(autoincrement())
  name                  String
  group_id              Int
  unlock_timeout        Int
  first_response_time   Int
  first_response_notify Int
  update_time           Int
  update_notify         Int
  solution_time         Int
  solution_notify       Int
  system_address_id     Int
  calendar_name         String
  default_sign_key      String
  salutation_id         Int
  signature_id          Int
  follow_up_id          Int
  follow_up_lock        Int
  comments              String
  valid_id              Int
  create_time           DateTime
  create_by             Int
  change_time           DateTime
  change_by             Int
  ticket                ticket[]

  follow_up_possible follow_up_possible @relation(fields: [follow_up_id], references: [id])
  permission_groups  permission_groups  @relation(fields: [group_id], references: [id])
  valid              valid              @relation(fields: [valid_id], references: [id])
  salutation         salutation         @relation(fields: [salutation_id], references: [id])
  system_address     system_address     @relation(fields: [system_address_id], references: [id])
  signature          signature?         @relation(fields: [signature_id], references: [id])

  users   users? @relation(fields: [usersId], references: [id])
  usersId Int?
}

